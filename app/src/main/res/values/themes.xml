<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.GreenSpace" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/green</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <item name="colorButtonNormal">@color/green</item>
        <item name="android:colorButtonNormal">@color/green</item>
        <item name="colorPrimaryVariant">@color/green_dark</item>
        <item name="colorAccent">@color/soft_green</item>
        <item name="android:colorBackground">@color/white</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
